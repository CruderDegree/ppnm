set term svg background rgb "white"
set out "Plot-A.svg"
set title "Wave functions for a quantum particle in a box"
set encoding utf8
set xlabel "ξ"
set ylabel "ψ"
set xrange [0:1]
set key bottom right
plot 	"data-A.txt" using 1:2 with lines linestyle 1 title "ψ_1" ,\
	"" using 1:3 with lines linestyle 2 title "ψ_2" ,\
	"" using 1:4 with lines linestyle 3 title "ψ_3" ,\
	"" using 1:5 with lines linestyle 4 title "ψ_4" ,\
	"" using 1:6 with lines linestyle 5 title "ψ_5"
